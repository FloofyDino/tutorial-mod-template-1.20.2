{
	"schemaVersion": 1,
	"id": "tutorial-mod",
	"version": "${version}",
	"name": "tutorial Mod",
	"description": "This is a tutorial mod by twig",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/tutorial-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.twig.tutorialmod.TutorialMod"
		],
		"client": [
			"net.twig.tutorialmod.TutorialModClient"
		],
		"fabric-datagen": [
			"net.twig.tutorialmod.TutorialModDataGenerator"
		]
	},
	"mixins": [
		"tutorial-mod.mixins.json",
		{
			"config": "tutorial-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}